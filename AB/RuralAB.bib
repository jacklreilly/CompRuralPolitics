	Month = apr,
	Number = {2},
	Pages = {194--204},
	Title = {Second home countryside. {Representations} of the rural in {Finnish} popular discourses},
	Url = {http://www.sciencedirect.com/science/article/pii/S0743016709000473},
	Urldate = {2018-10-18},
