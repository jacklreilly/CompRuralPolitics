	Issn = {0962-6298},
	Journal = {Political Geography},
	Month = jan,
	Number = {1},
	Pages = {99--119},
	Shorttitle = {Politics and regions in {Korea}},
	Title = {Politics and regions in {Korea}: an analysis of the recent presidential election},
